class Point {
   has x : Number = 0
   has y : Number = 0

   static has DEBUG : Boolean = false

   static method apply() {
      self.new
   }

   method init() {
      print("HELLS BELLS", self, Point3D)
   }
   method move(x = 0, y = 0) : Void {
      .x = x
      .y = y
   }
}

class Point3D from Point {
   has z : Number = 0
   method greet() {
      print("Hello World!")
   }

   method >>+<<(b) {
      print("funny operator:", b)
      print("TRACE:", trace())
   }
   method move(x = 0, y = 0, z = 0) : Void {
      super.move(x, y)
      .z = z
   }
}

