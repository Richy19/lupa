
class Point {
    has x = 0
    has y = 0

    class Inner {
        method greet(whom) {
            print("Hello ${whom}!")
        }
    }
    method __init() {
        var i = Inner()
        i.greet("World")
    }
    method move(x, y) {
        self.x = x
        self.y = y
    }
}

class Point3D from Point {
    has z = 0

    method move(x, y, z) {
        super::move(self, x, y)
        self.z = z
    }
}

var p = Point3D()
print(Point::Inner())
p.move(11,22,33)
print(p.x, p.y, p.z)

