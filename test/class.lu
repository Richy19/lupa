
class Point {
    has x = 0
    has y = 0

    class Inner {
        method greet(whom) {
            print("Hello ${whom}!")
        }
    }
    method __init() {
        var i = Inner.new()
        i.greet("World")
    }
    method move(x, y) {
        self.x = x
        self.y = y
    }
}

class Point3D from Point {
    has z = 0

    method move(x, y, z) {
        super::move(self, x, y)
        self.z = z
    }
}

var p = Point3D.new()
assert(Point.Inner)
for i=1, 1_000 {
   p.move(i+1, i+2, i+3)
}
print(p.x, p.y, p.z)

