
class Point {
   has x : Number = 0
   has y : Number = 0

   method __init() {
      print("class => ", self)
   }
   method move(x = 0, y = 0) : Void {
      self.x = x
      self.y = y
   }
}

class Point3D from Point {
   has z : Number = 0

   method greet() {
      print("Hello World!")
   }

   method move(x = 0, y = 0, z = 0) : Void {
      self.Point.move(x, y)
      self.z = z
   }
}

var p = Point()
var p = Point3D()

print("HERE:", p)
p.move()
p.greet()
print("HERE2")

assert(p.isa(Point3D))
assert(p.isa(Point))

for i = 1, 10_000_000 {
   p.move(i+1, i+2, i+3)
}

print("HERE:", p)

for k,v in p {
   print("${k} => ${v}")
}

try {
   p.z = "cheese"
}
catch (e : TypeError) {
   print("caught => ${e}")
}

