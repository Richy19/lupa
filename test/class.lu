class Point {
   has x : Number = 0
   has y : Number = 0

   method init() {
      print("HELLS BELLS", self)
   }
   method move(x = 0, y = 0) : Void {
      .x = x
      .y = y
   }
}

class Point3D from Point {
   has z : Number = 0

   method greet() {
      print("Hello World!")
   }

   method >>+<<(b) {
      print("funny operator:", b)
   }
   method move(x = 0, y = 0, z = 0) : Void {
      super.move(x, y)
      .z = z
   }
}

var p = Point.new
var p = Point3D.new

var a = p >>+<< 42

print("HERE:", p)
p.move()
p.greet()
print("HERE2")

assert(p.isa(Point3D))
assert(p.isa(Point))
print(typeof p)

for i = 1, 10_000_000 {
