package Acme {

    class Point {
        has x : Number = 0
        has y : Number = 0

        method move(x, y) {
            self.x = x
            self.y = y
        }
    }

    class Point3D from Point {
        has z : Number = 0

        method move(x, y, z) {
            super::move(self, x, y)
            self.z = z
        }
    }
}

var p = Acme.Point3D.new()
