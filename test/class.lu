
class Point {
    has x : Number = 0
    has y : Number = 0

    method __init() {
        print("class => ", Point)
    }
    method move(x = 1, y = 1) : Void {
        self.x = x
        self.y = y
        return
    }
}

class Point3D from Point {
    has z : Number = 0

    function greet() {
       print("Hello World!")
    }

    method move(x : Number = getit(), y : Number = 0, z : Number = 0) : Void {
        super::move(self, x, y)
        self.z = z
        return 42
    }

    function getit() {
       return 4
    }
}

var p = Point()

p.move()
print(p.x, p.y)

var p = Point3D()
print(p, Point3D)

p.move()
assert(p.x == 4)

for i = 1, 10_000_000 {
   p.move(i+1, i+2, i+3)
}
print(p.x, p.y, p.z)

try {
   p.z = "cheese"
}
catch (e : String) {
   print(e)
}
catch (e : Number) {
   print(e)
}

