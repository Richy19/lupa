
class Point {
    has x = 0
    has y = 0

    method __init() {
        print("class => ", Point)
    }
    method move(x = 1, y = 1) : Void {
        self.x = x
        self.y = y
        return
    }
}

class Point3D from Point {
    has z = 0

    function greet() {
       print("Hello World!")
    }

    method move(x, y, z) {
        super::move(self, x, y)
        self.z = z
    }
}

var p = Point()

p.move()
print(p.x, p.y)

var p = Point3D()

for i = 1, 10_000_000 {
   p.move(i+1, i+2, i+3)
}
print(p.x, p.y, p.z)

