
class Point {
    has x = 0
    has y = 0

    method __init() {
        print("class => ", Point)
    }
    method move(x, y) {
        self.x = x
        self.y = y
    }
}

class Point3D from Point {
    has z = 0

    method move(x, y, z) {
        super::move(self, x, y)
        self.z = z
    }
}

var p = Point3D()
for i=1, 10_000_000 {
   p.move(i+1, i+2, i+3)
}
print(p.x, p.y, p.z)

