object Point {
    has x = 0
    has y = 0
    method move(x, y) {
        self.x = x
        self.y = y
    }
}

var p = Point
print(p.x, p.y)

p.move(11,22)
print(p.x, p.y)
print(Point.x, Point.y)

object Point3D from Point {
    has z = 0
    method move(x, y, z) {
        super::move(self, x, y)
        self.z = z
    }
    method greet() {
        print("Hi!")
    }
}

var p2 = Point3D
print(p2.x,p2.y,p2.z)
p2.greet()
p2.move(11,22,33)
print(p2.x,p2.y,p2.z)

