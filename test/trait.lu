
trait Guide[T] {
   method walk(m : T) {
      print(m)
   }
}

trait Pet {
   has size : Number = 42
   needs legs
   method wooz() {
      var x = 42
      print(x)
   }
}

guard Int(i : Number) {
   return math#floor(i)
}

class Mammal {
   has blood = "warm"
}

class Dog from Mammal with Guide[String], Pet {
   has legs : Int = 4
   has name : String = 'anon'

   method greet(mesg : Array = [ 'asdfas' ]) {
      print("woof!")
      print("Size => ", self.size)
      print("BLOOD => ", self.blood)
   }
}

var d : Dog = Dog.new
d.walk("walkies!")
d.greet
d.wooz
d.legs = 4.2
print("LEGS:", d.legs)
d.name = "Fido"

print(d is Dog)
print(d does Pet)

