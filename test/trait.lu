
trait Guide(M) {
    method walk() { print(M) }
}

trait Pet(N) {
    has size = N
    needs legs
    method wooz() {
        var x = 42
        print(x)
    }
}

class Mammal {
    has blood = "warm"
}

class Dog from Mammal with Guide("guiding"), Pet(42) {
    has legs : Number
    has name : String

    method greet(mesg : Array = [ 'asdfas' ]) {
        print("woof!")
        print("Size => ", self.size)
        print("BLOOD => ", self.blood)
    }
}

var d = Dog.new
d.walk
d.greet
d.wooz
d.legs = 4
d.legs = nil
d.name = "Fido"

print(d is Dog)
print(d does Pet)

