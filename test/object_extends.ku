
object Point {
    has x = 0
    has y = 0
    method move(x, y) {
        self.x = x
        self.y = y
    }
}

object Point3D from Point {
    has z = 0
    method move(x, y, z) {
        super::move(self, x, y)
        self.z = z
    }
}

print(Point)
print(Point3D)
var p = Point3D

p.move(11,22,33)
for i=1, 10000000 {
    p.move(i, i+1, i+2)
}
print(p.x, p.y, p.z)
print(p)

