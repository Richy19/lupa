class Point {
    has x = 0
    has y = 0

    method __init() {
        print(self, "Point.init")
    }

    method move(x, y) {
        self.x = x
        self.y = y
    }
}


var p = Point()
print("ISA:", p.isa(Point))
print("CAN:", p.can('move'))

class Point3D from Point {
    has z = 0

    method __init() {
        super::__init(self)
        print(self, "Point3D.init")
    }

    method move(x, y, z) {
        super.move(x, y)
        self.z = z
    }
}

var p = Point3D()
p.move(11,22,33)

