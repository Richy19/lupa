class Point {
    has x = 0
    has y = 0

    method __init() {
        print(self, "Point.init")
    }

    method move(x, y) {
        self.x = x
        self.y = y
    }
}


var p = Point.new()
print("ISA:", p.isa(Point))
print("CAN:", p.can('move'))

class Point3D extends Point {
    has z = 0

    method __init() {
        super::__init(self)
        print(self, "Point3D.init")
    }

    method move(x, y, z) {
        super.move(x, y)
        self.z = z
    }
}

var p = Point3D.new()
for i=1, 10000000 {
    p.move(i, i+1, i+2)
}

