function Int(n : Number) {
   if !n.isa(Number) {
      error("TypeError: not a number")
   }
   return math::floor(n)
}

var i1 : Int = "69"
print(i1)
i1 += 42
print(i1)

var f = function(a : Int) {
   print("got:", a)
}

f(101)

class Point {
   has x : Int = 0
   has y = 0

   method move(x, y) {
      self.x = x
      self.y = y
   }
}

var p = Point()
p.move(1, 2)
