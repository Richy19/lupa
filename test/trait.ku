trait Guide<M> {
    method walk() { print(M) }
}

trait Pet<N> {
    has size = N

    method wooz() {
        var x = N
        print(x)
    }
}

class Mammal {
    has blood = "warm"
}

class Dog extends Mammal with Guide<"guiding"> with Pet<42> {
    self::DEBUG = true

    method greet() {
        print("woof!")
        print("Size => ", self.size)
        print("BLOOD => ", self.blood)
    }
}

var d = Dog.new()
d.walk()
d.greet()
d.wooz()

print(d.isa(Dog))

