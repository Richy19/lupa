class Point {
    has x : Number = 0
    has y : Number = 0
    method move(x : Number, y : Number) {
        self.x = x
        self.y = y
    }
}

class Point3D from Point {
    has z : Number = 0
    method move(x : Number, y : Number, z : Number) {
        super.move(x, y)
        self.z = z
    }
}

var p = Point3D.new
for i=1,100_000_000 {
    p.move(i, i+1, i+2)
}
print(p.x, p.y, p.z)

